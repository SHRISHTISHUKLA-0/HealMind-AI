# API Configuration
REACT_APP_API_URL=http://localhost:3000
REACT_APP_WEBSOCKET_URL=ws://localhost:3000

# Feature Flags
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_VOICE=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# External Services
